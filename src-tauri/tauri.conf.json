{
  "productName": "UE-Rush",
  "version": "0.1.0",
  "identifier": "com.uerush.app",
  "build": {
    "beforeDevCommand": "trunk serve --port 1420",
    "beforeBuildCommand": "trunk build --release",
    "devPath": "http://localhost:1420",
    "distDir": "../src/dist"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterBundle": false,
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": null
    },
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "",
      "exceptionDomain": "",
      "signingIdentity": null,
      "providerShortName": null,
      "entitlements": null
    },
    "linux": {
      "deb": {
        "depends": []
      },
      "appimage": {
        "bundleMediaFramework": false
      }
    }
  },
  "plugins": {
    "updater": {
      "active": false
    }
  },
  "capabilities": [
    {
      "identifier": "microphone",
      "windows": ["Record"],
      "macOS": ["Microphone"],
      "linux": ["Microphone"]
    }
  ]
}
